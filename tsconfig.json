{
  "compilerOptions": {
    "target":"es6",
    "module": "commonJs",
    "lib": [        "es5",
      "es6",
      "es7",
      "es2015.core",
      "es2015.collection",
      "es2015.generator",
      "es2015.iterable",
      "es2015.promise",
      "es2015.proxy",
      "es2015.reflect",
      "es2015.symbol",
      "es2015.symbol.wellknown",
      "esnext.asynciterable",
      "es2017.object",
    "dom"],
    "types": [ "node", "mocha", "chai" ],
    "declaration": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "strict": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": false,
    "inlineSourceMap": true,
    "inlineSources": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization":false,
    "esModuleInterop": true
  }
//  "include": [
//    "src/**/*.ts"
//  ]
}
